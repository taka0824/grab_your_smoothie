<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <div class="page-title">
        <h2><span>EDIT YOUR PROFILE</span></h2>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-4 col-xs-offset-4">
      <%= form_with model: @end_user, url: end_users_end_user_path(@end_user), local: true do |f| %>
        <%= render partial: "layouts/error_message", locals: { model: f.object } %>
        <table class="table">
          <tr>
            <th><%= f.label :name, "アカウント名" %></th>
            <td><%= f.text_field :name %></td>
          </tr>
          <tr>
            <th><%= f.label :email, :"メールアドレス" %></th>
            <td><%= f.email_field :email %></td>
          </tr>
        </table>
        <%= f.submit "変更", class: "btn btn-primary" %>
      <% end %>
      <br>
      <%= link_to destroy_confirm_end_users_end_users_path(current_end_user) do %>
        <button class="btn btn-danger">退会する</button>
      <% end %>
    </div>
  </div>
</div>